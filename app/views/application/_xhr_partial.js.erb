ujs.closeBsModals();
ujs.closeRemodals();

if ($('#<%= params[:block] || dashed(params[:partial]) %>').length) {
  ujs.html('#flashes', '<%= j render("flashes") %>');
  ujs.replace('#<%= params[:block] || dashed(params[:partial]) %>', '<%= j render(params[:partial]) %>');
} else {
  ujs.visit('<%= url %>');
}
